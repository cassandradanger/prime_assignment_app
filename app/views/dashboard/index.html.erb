<div class="row">
  <div class="col-md-3 col-xs-12">
    <div class="ibox">
      <div class="ibox-title"><h5>Users</h5></div>
      <div class="ibox-content">
        <h1 class="no-margin"><%= @user_count %></h1>
        <small>Total Users</small>
      </div>
    </div>
  </div>


  <div class="col-md-3 col-xs-12">
    <div class="ibox">
      <div class="ibox-title"><h5>Started</h5></div>
      <div class="ibox-content">
        <h1 class="no-margin"><%= @apps_count %></h1>
        <small>Started Applications</small>
      </div>
    </div>
  </div>


  <div class="col-md-3 col-xs-12">
    <div class="ibox">
      <div class="ibox-title"><h5>Complete</h5></div>
      <div class="ibox-content">
        <h1 class="no-margin"><%= @apps_completed_count %></h1>
        <small>Completed Applications</small>
      </div>
    </div>
  </div>

  <div class="col-md-3 col-xs-12">
    <div class="ibox">
      <div class="ibox-title"><h5>Accepted</h5></div>
      <div class="ibox-content">
        <h1 class="no-margin"><%= @apps_accepted_count %></h1>
        <small>Accepted Applications</small>
      </div>
    </div>
  </div>
</div>

