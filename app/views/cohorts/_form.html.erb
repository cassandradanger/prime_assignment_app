<%= simple_form_for(@cohort) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :prework_start %>
    <%= f.input :classroom_start %>
    <%= f.input :graduation %>
    <%= f.input :applications_open %>
    <%= f.input :applications_close %>
    <%= f.input :target_size %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
