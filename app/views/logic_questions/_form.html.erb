<%= bootstrap_form_for([@course,@logic_question]) do |f| %>

    <%= f.text_area :question, rows: 5 %>
    <%= image_tag @logic_question.question_image.url %>
    <%= f.file_field :question_image %>
    <%= f.text_field :solution %>
    <%= f.text_field :position %>
    <%= f.check_box :published %>
    <%= f.submit "Save" %>

<% end %>
